{# Case 1: From widget or search page #}
{% block element_product_listing_wrapper %}
    {% sw_include '@Storefront/storefront/element/cms-element-sidebar-filter.html.twig' with {
        listing: moorlSearchResult,
        sidebar: false
    } %}

    {% sw_include '@Storefront/storefront/element/cms-element-merchant-listing.html.twig' with {
        searchResult: moorlSearchResult,
        config: moorlSearchResult.elementConfig,
        dataUrl: url('widgets.search.pagelet.v2'),
        filterUrl: url('widgets.search.filter'),
        sidebar: 0,
        boxLayout: 'minimal',
        displayMode: ''
    } %}
{% endblock %}
